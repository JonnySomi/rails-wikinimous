<h3><%= @article.title %></h3>
<p><%= @article.content %></p>

<p><%= link_to 'Back to Articles List', articles_path %></p>
<p><%= link_to 'Edit Article', edit_article_path %></p>
<p><%= link_to "Delete #{@article.title}?",
                article_path(@article),
                method: :delete,
                data: { confirme: "Are you sure?"}%></p>
